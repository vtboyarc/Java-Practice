class Dog {
	String type;
	String furColor;
	int age;

	void bark() {
		System.out.println("woof!");
	}

	String getType() {
		return type;
	}

	String getFurcColor() {
		return furColor;
	}

	int getAge() {
		return age;
	}
}

public class App {
	public static void main(String[] args) {

		Dog bonnie = new Dog();
		bonnie.age = 8;
		bonnie.furColor = "brown and white";
		bonnie.type = "collie";
		int age = bonnie.getAge();
		System.out.println(age);

	}
}
