class Robot {
	String name;
	int age;
	String type;
	
	public void setName(String name) {
		this.name = name;
	}
	
	public void setAge(int age) {
		this.age = age;
	}
	
	public void setType(String type) {
		this.type = type;
	}
	
	public int getAge() {
		return age;
	}
	
	public String getName() {
		return name;
	}
	
	public String getType() {
		return type;
	}
}

public class App {
	public static void main(String[] args) {
		
	}
}
