public class App {
	public static void main(String[] args) {
		
		//one dimension array
		int[] values = {3, 5, 2343};
		
		System.out.println(values[2]);
		
		//multi-dimensional, each element in the array, is another array
		int[][] grid = {
				{3, 5, 2343}, //row 0
				{2, 4}, //row 1
				{1,2,3,4} //row 2
		};
		//so row 1, index 1
		System.out.println(grid[1][1]);
		//row 0, index 2
		System.out.println(grid[0][2]);
	}
}
