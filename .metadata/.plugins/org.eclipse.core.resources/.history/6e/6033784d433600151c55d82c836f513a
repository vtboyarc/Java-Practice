public class App {

	public static void main(String[] args) {

		//once you've created a string, cna't every change it, immutable. 
		//this all gets added to the info var as one String
		String info = "";
		info += "My name is Adam";
		info += " ";
		info += "I am a builder";
		
		System.out.println(info);
		
	}

}
