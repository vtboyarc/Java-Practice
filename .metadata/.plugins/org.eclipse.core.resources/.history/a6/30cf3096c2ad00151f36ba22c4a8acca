package com;

import java.io.*;

public class Treets {
	// method "save" that takes an array of Treet called treets
	public static void save(Treet[] treets) {
		try (FileOutputStream fos = new FileOutputStream("treets.serial");
				ObjectOutputStream oos = new ObjectOutputStream(fos);
				) {
				oos.writeObject(treets);
		} catch(IOException ioe) {
			System.out.println("Problem saving");
			ioe.printStackTrace();
		} 
	}
	
	public static Treet[] load() {
		Treet[] treets = new Treet[0];
		
		return treets;
	}
}
