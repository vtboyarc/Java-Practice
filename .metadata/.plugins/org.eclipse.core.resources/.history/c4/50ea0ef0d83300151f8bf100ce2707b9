import java.util.Scanner;

class Person {
	String name;
	int age;

}

public class App {
	public static void main(String[] args) {

		Scanner scanner = new Scanner(System.in);

		String name;

		do {
			System.out.println("What is your name?");

			name = scanner.nextLine();
		}

		while (name != "Adam");

		System.out.println("Sorry, you lose");

	}
}
